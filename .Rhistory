for (i in seq_along(thresholds)) {
t <- thresholds[i]
pred <- ifelse(framingham_scaled$chd_prob > t, 1, 0)
cm <- table(Predicted = pred, Actual = framingham_scaled$TenYearCHD)
TP <- cm["1", "1"]
TN <- cm["0", "0"]
FP <- cm["1", "0"]
FN <- cm["0", "1"]
prec <- TP / (TP + FP)
rec  <- TP / (TP + FN)
f1   <- 2 * prec * rec / (prec + rec)
results$Precision[i] <- prec
results$Recall[i] <- rec
results$F1[i] <- f1
}
print(results)
